# Copy this file to .env.local and customize for your environment

# App Configuration
NODE_ENV=production
VITE_APP_NAME=Prospectio Frontend
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_URL=http://localhost:3000/api
VITE_API_TIMEOUT=10000

# Database Configuration (if using backend services)
POSTGRES_DB=prospectio
POSTGRES_USER=prospectio
POSTGRES_PASSWORD=prospectio_password

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Security
VITE_APP_SECRET_KEY=your-secret-key-here

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false

# External Services
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=
